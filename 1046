#include <stdio.h>

int main() {
    int inicio, fim, duracao;

    scanf("%d %d", &inicio, &fim);

    if (fim > inicio) {
        duracao = fim - inicio;
    } else {
        duracao = 24 - inicio + fim;
    }

    printf("O JOGO DUROU %d HORA(S)\n", duracao);

    return 0;
}
